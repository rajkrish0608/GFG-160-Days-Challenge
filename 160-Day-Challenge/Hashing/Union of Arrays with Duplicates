class Solution {
    public static int findUnion(int a[], int b[]) {
         HashSet<Integer> unionSet = new HashSet<>();

        for (int num : a) {
            unionSet.add(num);
        }

        for (int num : b) {
            unionSet.add(num);
        }

        return unionSet.size();
        
    }
}
